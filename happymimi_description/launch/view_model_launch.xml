<?xml version='1.0'?>
<launch>
  <arg name="urdf_file" default="$(command 'xacro $(find-pkg-share happymimi_description)/robots/vhappymimi.urdf.xacro')"/>
  <arg name="robot_description" default="$(var urdf_file)"/>

  <node pkg="robot_state_publisher" exec="robot_state_publisher" output="screen">
    <param name="robot_description" value="$(var robot_description)"/>
    <param name="publish_frequency" value="5.0"/>
  </node>
  <node pkg="joint_state_publisher_gui" exec="joint_state_publisher_gui" output="screen">
  </node>

  <!--<node pkg="rviz" exec="rviz" args="-d $(find hiyoko_description)/launch/model.rviz"/> -->
</launch>
